{
  "name": "broadcast-server",
  "version": "1.0.0",
  "description": "透過瀏覽器收聲音，並且送到後端，再由後端廣播出去",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:lib": "babel public/ -d lib/",
    "html": "browserify lib/index.js -t babelify --outfile dist/recorder.js -s Recorder",
    "prestart": "npm run html",
    "start": "node ./src/app.js"
  },
  "author": "jeni",
  "license": "ISC",
  "dependencies": {
    "audiobuffer": "^0.2.0",
    "audiobuffer-to-wav": "^1.0.0",
    "browserify": "^17.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "inline-worker": "^1.1.0",
    "node-fetch": "^2.6.1",
    "ws": "^7.4.2"
  },
  "devDependencies": {
    "babelify": "^10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/s7130457/broadcast-server.git"
  },
  "keywords": [
    "broadcast",
    "recorderjs"
  ],
  "bugs": {
    "url": "https://github.com/s7130457/broadcast-server/issues"
  },
  "homepage": "https://github.com/s7130457/broadcast-server#readme",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  }
}
